Proceso replica4_DolaresaPesos
	Definir pesos, dolares Como Entero;
	Escribir "Convertidor de Dolares a Pesos Mexicanos";
	Escribir "Dolares $19";
	Escribir "Ingrese la cantidad de dolares";
	Leer dolares;
	pesos<-dolares*19;
	Escribir "La conversion es: ",pesos," Pesos Mexicanos";
FinProceso
