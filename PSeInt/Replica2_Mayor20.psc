Proceso Replica2_Mayor20
	Definir num1, num2, num3, res Como Real;
	Escribir "Escribe el primer numero: ";
	Leer num1;
	Escribir "Escribe el segundo numero: ";
	Leer num2;
	Escribir "Escribe el tercer numero: ";
	Leer num3;
	res<-num1+num2+num3;
	si res>20 Entonces
		Escribir "Tu numero es mayor a 20";
	FinSi
FinProceso
